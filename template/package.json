{
  "name": "{{ name }}",
  "version": "1.0.0",
  "description": "{{ description }}",
  "author": "{{ author }}",
  "private": true,
  "engines": {
    "node": ">= 6.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production npm run clean && npm run sass && npm run build:parts && npm run clean:css && npm run process:html",
    "build:parts": "npm run sass && npm run compile",
    "clean": "rimraf dist",
    "clean:css": "cleancss -o dist/css/main.min.css src/css/main.css",
    "compile": "webpack --config config/webpack.config.prod.js",
    "dev": "cross-env NODE_ENV=development npm run sass && concurrently --kill-others \"webpack-dev-server --hot --inline\" \"node-sass --watch --output ./src/css ./src/sass\"",
    "process:html": "htmlprocessor src/index.html -o dist/index.html",
    "sass": "node-sass --output ./src/css ./src/sass"
  },
  "dependencies": {
    "vue": "^2.3.3",
    "vue-class-component": "^5.0.2",
    "vue-property-decorator": "^5.1.1",
    "vue-router": "^2.6.0"
  },
  "devDependencies": {
    "@types/node": "^8.0.15",
    "autoprefixer": "^7.1.2",
    "awesome-typescript-loader": "^3.2.1",
    "chalk": "^2.0.1",
    "chromedriver": "^2.27.2",
    "clean-css": "^4.1.7",
    "clean-css-cli": "^4.1.6",
    "compression-webpack-plugin": "^1.0.0",
    "concurrently": "^3.5.0",
    "connect-history-api-fallback": "^1.3.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^5.0.1",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.0",
    "cssnano": "^3.10.0",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.14.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.1.3",
    "html-webpack-plugin": "^2.28.0",
    "htmlprocessor": "^0.2.4",
    "http-proxy-middleware": "^0.17.3",
    "loglevel": "^1.4.1",
    "node-sass": "^4.5.3",
    "opn": "^5.1.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "ora": "^1.2.0",
